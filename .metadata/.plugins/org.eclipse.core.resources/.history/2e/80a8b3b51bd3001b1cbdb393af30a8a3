
public class Overridings {

	void show (int a ,String b)
	{
		System.out.println("1");
	}
	
}

class Overriding1
{

	void show ()
	{
		System.out.println("2");
		
	}
	

public static void main(String[] args) 
	{
		// TODO Auto-generated method stub

	
	Overridings or1=new Overridings();
			or1.show(10,"rakesh");
	}

}
